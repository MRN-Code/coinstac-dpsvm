{
  "meta": {
    "name": "Differentially Private Support Vector Machines demo",
    "id": "dpsvm",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac_dpSVM",
    "description": "A test for differentially-private support vector machines"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "dpsvm",
    "command": [
      "python",
      "\/computation\/scripts\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "dpsvm",
      "command": [
        "python",
        "\/computation\/scripts\/remote.py"
      ]
    },
    "input": {
      "lambda":
      {
        "defaultValue": 0,
        "label": "Lambda",
        "max": 1,
        "min": 0,
        "step": 0.05,
        "type": "number",
        "source": "owner"
      },
      "train_test_split":
      {
        "defaultValue": 0.33,
        "label": "Train/Test Split",
        "max": 1,
        "min": 0,
        "step": 0.01,
        "type": "number",
        "source": "owner"
      },
      "covariates":
      {
        "label": "Covariates",
        "type": "array",
        "items": ["boolean", "number"]
      },
      "data": {
        "label": "Data",
        "type": "array",
        "items": ["boolean"]
      }
    },
    "output": {
      "regressions": {
        "label": "Error/Accuracy",
        "type": "number"
      }
    },
    "display": [
    ]
  }
}
